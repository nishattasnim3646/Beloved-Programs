#include <bits/stdc++.h>
using namespace std;

int main() {
    int n, m;
    cin >> n >> m;
    vector<int> arr(n);
    unordered_map<int, int> mp;

    for (int i = 0; i < n; ++i) {
        cin >> arr[i];
        mp[arr[i]]++;
    }

    for (int i = 0; i < n; ++i) {
        int temp = m - arr[i];
        if (mp[temp] > 0 && !(arr[i] == temp && mp[temp] == 1)) {
            cout << "YES" << endl;
            return 0;
           
        }
    }

    cout << "NO" << endl;
}
