#include<bits/stdc++.h>
using namespace std;
 
int main()
{
	int n, count = 0;
	cin >> n;
	string s, t, tmp;
	cin >> s >> t;
	for(int i = 0; i < n; i++)
	{
        string tmp;
        for(int j=i; j<n; ++j){
            tmp+=s[j];
        }
		if(t.find(tmp) == 0)
		{
			cout << count << endl;
			return 0;
		}
		count++;
       // cout<<tmp<<endl;
	}
  
}
